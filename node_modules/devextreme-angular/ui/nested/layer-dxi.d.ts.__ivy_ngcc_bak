import Store from 'devextreme/data/abstract_store';
import DataSource, { Options as DataSourceOptions } from 'devextreme/data/data_source';
import { Font } from 'devextreme/viz/core/base_widget';
import { NestedOptionHost } from 'devextreme-angular/core';
import { CollectionNestedOption } from 'devextreme-angular/core';
export declare class DxiLayerComponent extends CollectionNestedOption {
    borderColor: string;
    borderWidth: number;
    color: string;
    colorGroupingField: string | undefined;
    colorGroups: Array<number>;
    customize: Function;
    dataField: string | undefined;
    dataSource: any | DataSource | DataSourceOptions | Store | null | string | Array<any>;
    elementType: string;
    hoveredBorderColor: string;
    hoveredBorderWidth: number;
    hoveredColor: string;
    hoverEnabled: boolean;
    label: {
        dataField?: string;
        enabled?: boolean;
        font?: Font;
    };
    maxSize: number;
    minSize: number;
    name: string;
    opacity: number;
    palette: string | Array<string>;
    paletteIndex: number;
    paletteSize: number;
    selectedBorderColor: string;
    selectedBorderWidth: number;
    selectedColor: string;
    selectionMode: string;
    size: number;
    sizeGroupingField: string | undefined;
    sizeGroups: Array<number>;
    type: string;
    protected readonly _optionPath: string;
    constructor(parentOptionHost: NestedOptionHost, optionHost: NestedOptionHost);
    ngOnDestroy(): void;
}
export declare class DxiLayerModule {
}
